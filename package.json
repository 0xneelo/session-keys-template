{
  "name": "@symmio/session-keys",
  "version": "1.0.0",
  "description": "Browser-based session key management for Symmio Protocol delegation",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src --ext .ts",
    "test": "vitest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "symmio",
    "session-keys",
    "ethereum",
    "delegation",
    "defi",
    "web3",
    "eip-712"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@noble/curves": "^1.3.0",
    "@noble/hashes": "^1.3.3",
    "@scure/bip32": "^1.3.3",
    "@scure/bip39": "^1.2.2",
    "ethers": "^6.11.0",
    "siwe": "^2.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "eslint": "^8.56.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "peerDependencies": {
    "ethers": "^6.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
